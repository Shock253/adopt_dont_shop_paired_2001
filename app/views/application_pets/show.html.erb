<%= @application.name %>
<%= @application.address %>
<%= @application.city %>
<%= @application.state %>
<%= @application.zip %>
<%= @application.phone_number %>
<%= @application.description %>

<% @application.pets.each do |pet| %>
  <section id="pet-<%= pet.id %>">
    <%= link_to pet.name, "/pets/#{pet.id}"%>
    <%= link_to "Approve", "/pets/#{pet.id}/applications/#{@application.id}", method: :post %>
  </section>
<% end %>
